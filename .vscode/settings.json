{
  "typescript.preferences.autoImportFileExcludePatterns": [
    "@remix-run/server-runtime",
    "@remix-run/router",
    "express",
    "@radix-ui/**",
    "@react-email/**",
    "react-router-dom",
    "react-router",
    "stream/consumers",
    "node:stream/consumers",
    "node:test",
    "console",
    "node:console"
  ],
  "workbench.editorAssociations": {
    "*.db": "sqlite-viewer.view"
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#2D1C76",
    "titleBar.activeBackground": "#56a627",
    "titleBar.activeForeground": "#FDFCFE",
    // tab colors
    "tab.activeBackground": "#04b3ee7e", // Background color of the active tab
    "tab.activeForeground": "#ffffff", // Text color of the active tab
    "tab.border": "#44475a", // Border color between tabs
    "tab.activeBorder": "#50fa7b"
  }
}
